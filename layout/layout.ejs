<%- partial('_partial/head') %>
<body>
  <script type="text/javascript">
    // fix sidebar flashing
    (function(){
      if (localStorage.getItem('sidebarState') == 'true') {
        document.body.className="sideShow"
      }
    })()
  </script>
  <div id="container">
    <%- partial('_partial/sidebar', null, {cache: !config.relative_link}) %>
    <div class="wrap">
      <%- partial('_partial/side') %>
      <%- partial('_partial/header', null, {cache: !config.relative_link}) %>
      <section id="main" class="content">
        <%- body %>
      </section>
    <%- partial('_partial/footer') %>
    </div>
  </div>
  <%- js('js/simple_dev') %>
  <% if (theme.duoshuo_shortname) { %>
    <%- partial('_script/comments/duoshuo.ejs') %>
  <% } else if (theme.disqus_shortname) { %>
    <%- partial('_script/comments/disqus.ejs') %>  
  <% } %>
</body>
</html>